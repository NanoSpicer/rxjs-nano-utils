{
  "name": "@nanospicer/rxjs-utils",
  "version": "0.0.8",
  "description": "A very small set of RxJs utilities that I often use on my Angular apps",
  "main": "./lib/index.js",
  "types": "./lib/types/index.d.ts",
  "files": [
    "lib",
    "src"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "start": "nodemon --exec ts-node src/index.ts",
    "clean": "rm -rf lib",
    "build": "npx tsc -p .",
    "release": "npm version patch && git push && npm run build && npm publish",
    "test": "npx jest",
    "test:watch": "npx jest --watch"
  },
  "dependencies": {
    "rxjs": "^6.6.0"
  },
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "jest": "^27.3.1",
    "jest-marbles": "^2.5.1",
    "nodemon": "^2.0.14",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NanoSpicer/rxjs-nano-utils.git"
  },
  "keywords": [
    "rxjs",
    "utils",
    "utilities",
    "typescript",
    "angular",
    "streams",
    "reactive",
    "pipes",
    "loading",
    "errors",
    "data"
  ],
  "author": "NanoSpicer",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/NanoSpicer/rxjs-nano-utils/issues"
  },
  "homepage": "https://github.com/NanoSpicer/rxjs-nano-utils#readme"
}
